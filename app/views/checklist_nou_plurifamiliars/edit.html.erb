<div id="modal-signatura-promotor" class="reveal-modal" data-reveal aria-labelledby="modalTitle" aria-hidden="true" role="dialog">
  <h3 id="modalTitle"><%= t('.signatura_promotor') %></h3>
  <div id="formulari-signatura-promotor" class="formulari-agent">
  </div>
  <a class="close-reveal-modal" aria-label="Close">&#215;</a>
</div>

<div class="background-seccio">
	<div class="row">
		<div class="large-12 columns">
			<div class="titol-seccio">
				<%= t('.titol_configuracio_document') %>
			</div>
		</div>
	</div>

	<div class="row">
		<div class="large-12 columns">
			
			<%= render 'form' %>
		</div>
	</div>

	<div class="row">
		<div class="large-12 columns">
			<div class="row">
				<div class="medium-6 columns">
					<div class="agents-container">
						<div class="agents-title">
							<%= t('.titol_promotor') %>
						</div>
						<div id="signatura-promotors">
							<%= render  :partial => '/signatura_promotors/llistat_promotors' %>
						</div>
						<div class="agents-footer">
							<div class="agents-footer-link right">
								<%= link_to (new_edifici_signatura_promotor_path), remote: true do %>
									<span class="suma">+</span> <%= t('.afegir_promotor') %>
								<% end %>
							</div>
						</div>
					</div>
				</div>
				<div class="medium-6 columns">
					
				</div>
			</div>
		</div>
	</div>


</div>