<%= simple_form_for(@declaracio) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :edifici_id %>
    <%= f.input :notari %>
    <%= f.input :n_protocol %>
    <%= f.input :data_declaracio %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
