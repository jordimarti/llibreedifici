<% @declaracions.each do |declaracio| %>
	<div class="dades-item">
		
		
	  <%= link_to edifici_declaracio_path(@edifici, declaracio), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="dades-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_declaracio_path(@edifici, declaracio), remote: true do %>
	    <div class="dades-editar">
	    	<%= t('.declaracio_obra_nova') %>
	    </div>
	  <% end %>
	</div>
<% end %>