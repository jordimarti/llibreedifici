<%= simple_form_for(@pagament) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :edifici_id %>
    <%= f.input :numorder %>
    <%= f.input :import %>
    <%= f.input :codi_retorn %>
    <%= f.input :codi_autoritzacio %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
