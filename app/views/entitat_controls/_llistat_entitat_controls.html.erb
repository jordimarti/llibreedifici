
<% @entitat_controls.each do |entitat| %>
	<div class="agents-item">
		
		
	  <%= link_to edifici_entitat_control_path(@edifici, entitat), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="agents-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_entitat_control_path(@edifici, entitat), remote: true do %>
	    <div class="agents-editar">
	    	<%= entitat.nom_entitat_control %>
	    </div>
	  <% end %>
	</div>
<% end %>
