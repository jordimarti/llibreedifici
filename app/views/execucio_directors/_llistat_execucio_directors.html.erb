
<% @execucio_directors.each do |execucio_director| %>
	<div class="agents-item">
		
		
	  <%= link_to edifici_execucio_director_path(@edifici, execucio_director), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="agents-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_execucio_director_path(@edifici, execucio_director), remote: true do %>
	    <div class="agents-editar">
	    	<%= execucio_director.nom_director_execucio %>
	    </div>
	  <% end %>
	</div>
<% end %>
