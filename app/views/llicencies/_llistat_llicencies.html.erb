<% @llicencies.each do |llicencia| %>
	<div class="dades-item">
		
		
	  <%= link_to edifici_llicencia_path(@edifici, llicencia), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="dades-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_llicencia_path(@edifici, llicencia), remote: true do %>
	    <div class="dades-editar">
	    	<%= llicencia.classe %>
	    </div>
	  <% end %>
	</div>
<% end %>