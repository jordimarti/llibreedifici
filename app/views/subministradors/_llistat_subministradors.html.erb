
<% @subministradors.each do |subministrador| %>
	<div class="agents-item">
		
		
	  <%= link_to edifici_subministrador_path(@edifici, subministrador), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="agents-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_subministrador_path(@edifici, subministrador), remote: true do %>
	    <div class="agents-editar">
	    	<%= subministrador.nom_subministrador %>
	    </div>
	  <% end %>
	</div>
<% end %>
