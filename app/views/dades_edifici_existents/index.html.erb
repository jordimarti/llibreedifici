<h1>Listing Dades Edifici Existents</h1>

<table>
  <thead>
    <tr>
      <th>Edifici</th>
      <th>Tipus via edifici</th>
      <th>Via edifici</th>
      <th>Numero edifici</th>
      <th>Bloc edifici</th>
      <th>Escala edifici</th>
      <th>Cp edifici</th>
      <th>Poblacio edifici</th>
      <th>Provincia edifici</th>
      <th>Ref cadastral</th>
      <th>Us edifici</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dades_edifici_existents.each do |dades_edifici_existent| %>
      <tr>
        <td><%= dades_edifici_existent.edifici_id %></td>
        <td><%= dades_edifici_existent.tipus_via_edifici %></td>
        <td><%= dades_edifici_existent.via_edifici %></td>
        <td><%= dades_edifici_existent.numero_edifici %></td>
        <td><%= dades_edifici_existent.bloc_edifici %></td>
        <td><%= dades_edifici_existent.escala_edifici %></td>
