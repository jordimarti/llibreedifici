        <td><%= dades_edifici_existent.poblacio_edifici %></td>
        <td><%= dades_edifici_existent.provincia_edifici %></td>
        <td><%= dades_edifici_existent.ref_cadastral %></td>
        <td><%= dades_edifici_existent.us_edifici %></td>
        <td><%= link_to 'Show', dades_edifici_existent %></td>
        <td><%= link_to 'Edit', edit_dades_edifici_existent_path(dades_edifici_existent) %></td>
        <td><%= link_to 'Destroy', dades_edifici_existent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
