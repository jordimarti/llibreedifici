
<% @industrials.each do |industrial| %>
	<div class="agents-item">
		
		
	  <%= link_to edifici_industrial_path(@edifici, industrial), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="agents-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_industrial_path(@edifici, industrial), remote: true do %>
	    <div class="agents-editar">
	    	<%= industrial.nom_industrial %>
	    </div>
	  <% end %>
	</div>
<% end %>
