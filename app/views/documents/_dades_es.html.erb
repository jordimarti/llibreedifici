<% @edifici.llicencies.each do |llicencia| %>
	<div class="taula-container">
		<p class="titol-taula">LICENCIAS:</p>
		<table class="taula-dades">
		  <tbody>
		    <tr>
		      <td class="taula-dos-columnes"><span class="titol-taula">Clase:</span> <%= llicencia.classe %></td>
		      <td class="taula-dos-columnes"><span class="titol-taula">Fecha:</span> <%= llicencia.data_llicencia %></td>
		    </tr>
		  </tbody>
		</table>
		<div class="espaiador"></div>
	</div>
<% end %>

