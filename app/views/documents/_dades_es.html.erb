<% @edifici.entitats.order(nom_entitat: :asc).each do |entitat| %>
	<div class="taula-container">
		<p class="titol-taula">ENTIDADES JURÍDICAS:</p>
		<table class="taula-dades">
		  <tbody>
		    <tr>
		      <td class="taula-dos-columnes"><span class="titol-taula">Nombre:</span> <%= entitat.nom_entitat %></td>
		      <td class="taula-dos-columnes"><span class="titol-taula">Superfície (m<sup>2</sup>):</span> <%= entitat.superficie %></td>
		    </tr>
		  </tbody>
		</table>
		<table class="taula-dades">
		  <tbody>
		    <tr>
		      <td class="taula-dos-columnes"><span class="titol-taula">Destinación:</span> <%= entitat.destinacio %></td>
		      <td class="taula-dos-columnes"><span class="titol-taula">Cuota (%):</span> <%= entitat.quota %></td>
		    </tr>
		  </tbody>
		</table>
		<div class="espaiador"></div>
	</div>
<% end %>


<% @edifici.energia_certificats.each do |energia_certificat| %>
	<div class="taula-container">
		<p class="titol-taula">CERTIFICACIÓN DE EFICIENCIA ENERGÉTICA:</p>
		<table class="taula-dades">
		  <tbody>
		    <tr>
		      <td class="taula-dos-columnes"><span class="titol-taula">Número de registro:</span> <%= energia_certificat.numero_registre %></td>
		      <td class="taula-dos-columnes"><span class="titol-taula">Fecha de finalización de la vigencia:</span> <%= energia_certificat.vigencia %></td>
		    </tr>
		  </tbody>
		</table>
		<table class="taula-dades">
		  <tbody>
		    <tr>
		      <td class="taula-dos-columnes"><span class="titol-taula">Consumo de energía primaria (kWh/a):</span> <%= energia_certificat.consum_energia_primaria %></td>
		      <td class="taula-dos-columnes"><span class="titol-taula">Emisiones de CO<sub>2</sub> (kg/m<sup>2</sup>a):</span> <%= energia_certificat.emissions_co2 %></td>
		    </tr>
		  </tbody>
		</table>
		<table class="taula-dades">
		  <tbody>
		    <tr>
		      <td class="taula-dos-columnes"><span class="titol-taula">Cualificación energética:</span> <%= energia_certificat.qualificacio_energetica %></td>
		      <td class="taula-dos-columnes"></td>
		    </tr>
		  </tbody>
		</table>
		<div class="espaiador"></div>
	</div>
<% end %>

