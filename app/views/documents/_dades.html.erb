<% @edifici.llicencies.each do |llicencia| %>
	<div class="taula-container">
		<p class="titol-taula">LLICÈNCIES:</p>
		<table class="taula-dades">
		  <tbody>
		    <tr>
		      <td class="taula-dos-columnes"><span class="titol-taula">Classe:</span> <%= llicencia.classe %></td>
		      <td class="taula-dos-columnes"><span class="titol-taula">Data:</span> <%= llicencia.data_llicencia %></td>
		    </tr>
		  </tbody>
		</table>
		<div class="espaiador"></div>
	</div>
<% end %>

