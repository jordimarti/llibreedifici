<%= simple_form_for(@control_entitat) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :edifici_id %>
    <%= f.input :nom_entitat_control %>
    <%= f.input :nif_entitat_control %>
    <%= f.input :tipus_via_entitat_control %>
    <%= f.input :via_entitat_control %>
    <%= f.input :numero_entitat_control %>
    <%= f.input :bloc_entitat_control %>
    <%= f.input :escala_entitat_control %>
    <%= f.input :pis_entitat_control %>
    <%= f.input :cp_entitat_control %>
    <%= f.input :poblacio_entitat_control %>
    <%= f.input :provincia_entitat_control %>
    <%= f.input :pais_entitat_control %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
