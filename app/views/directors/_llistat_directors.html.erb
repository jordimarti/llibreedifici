
<% @directors.each do |director| %>
	<div class="agents-item">
		
		
	  <%= link_to edifici_director_path(@edifici, director), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="agents-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_director_path(@edifici, director), remote: true do %>
	    <div class="agents-editar">
	    	<%= director.nom_director %>
	    </div>
	  <% end %>
	</div>
<% end %>
