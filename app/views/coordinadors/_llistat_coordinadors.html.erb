
<% @coordinadors.each do |coordinador| %>
	<div class="agents-item">		
		
	  <%= link_to edifici_coordinador_path(@edifici, coordinador), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="agents-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_coordinador_path(@edifici, coordinador), remote: true do %>
	    <div class="agents-editar">
	    	<%= coordinador.nom_coordinador %>
	    </div>
	  <% end %>
	</div>
<% end %>
