<p id="notice"><%= notice %></p>

<h1>Listing Edificis</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>User</th>
      <th>Tipus edifici</th>
      <th>Referencia cadastral</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @edificis.each do |edifici| %>
      <tr>
        <td><%= edifici.id %></td>
        <td><%= edifici.user_id %></td>
        <td><%= edifici.tipus_edifici %></td>
        <td><%= edifici.ref_cadastral %></td>
        <td><%= edifici.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>