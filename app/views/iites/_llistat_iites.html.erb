<% @iites.each do |iite| %>
	<div class="dades-item">
		
		
	  <%= link_to edifici_iite_path(@edifici, iite), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="dades-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_iite_path(@edifici, iite), remote: true do %>
	    <div class="dades-editar">
	    	<%= iite.data %>
	    </div>
	  <% end %>
	</div>
<% end %>