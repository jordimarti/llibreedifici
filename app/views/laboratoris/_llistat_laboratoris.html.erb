
<% @laboratoris.each do |laboratori| %>
	<div class="agents-item">
		
		
	  <%= link_to edifici_laboratori_path(@edifici, laboratori), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="agents-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_laboratori_path(@edifici, laboratori), remote: true do %>
	    <div class="agents-editar">
	    	<%= laboratori.nom_laboratori %>
	    </div>
	  <% end %>
	</div>
<% end %>
