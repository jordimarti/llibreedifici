<%= simple_form_for(@element_predefinit) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nom_element %>
    <%= f.input :sistema_element %>
    <%= f.input :descripcio_ca %>
    <%= f.input :descripcio_es %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
