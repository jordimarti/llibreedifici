
<% @projectistes.each do |projectista| %>
	<div class="agents-item">
		<%= projectista.nom_projectista %>
		
	  <%= link_to edifici_projectista_path(@edifici, projectista), remote: true, method: :delete,  data: { confirm: 'Segur que vols eliminar?' } do %>
		  <div class="agents-eliminar right">
		  	<span class="icon-cancel icones-icomoon"></span>
		  </div>
		<% end %>
		<%= link_to edit_edifici_projectista_path(@edifici, projectista), remote: true do %>
	    <div class="agents-editar right">
	    	<span class="icon-pencil icones-icomoon"></span>
	    </div>
	  <% end %>
	</div>
<% end %>
